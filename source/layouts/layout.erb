<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title><%= current_page.data.title %></title>
    <meta name="description" content="<%= current_page.data.description %>">
    <meta name="google-site-verification" content="<%= data.misc.google_search_console_validation %>">
    <meta name="viewport" content="width=device-width,minimum-scale=1">
    <link rel="canonical" href="<%= current_page.data.canonical_url %>">
    <link rel="stylesheet" href="https://use.typekit.net/oiy6mov.css">
    <%= stylesheet_link_tag 'site' %>
    <script>var clicky_site_ids = clicky_site_ids || []; clicky_site_ids.push(<%= data.misc.clicky %>);</script>
    <script async src="//static.getclicky.com/js"></script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
